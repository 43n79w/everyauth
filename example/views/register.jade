h2 Register
- if ('undefined' !== typeof errorMessage)
  #error= errorMessage
form(action: '/register', method: 'POST')
  #login
    label(for: 'login') Login
    input(type: 'text', name: 'login')
  #password
    label(for: 'password') Password
    input(type: 'password', name: 'password')
  #submit
    input(type: 'submit') Register
